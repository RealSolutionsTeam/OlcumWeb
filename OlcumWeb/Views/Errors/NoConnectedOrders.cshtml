<div style="text-align:center">
    <br />
    <br />
    <h2 class="swal2-title">

        Girilen Order numarasına ait veri bulunamadı.Bir sonraki adıma yönlendiriliyorsunuz..
    </h2>
</div>
<br />
<div class="progress">
    <div class="progress-bar progress-bar-striped bg-info" id="errorPageProgressBar" role="progressbar" style="width: 1%;" aria-valuenow="1" aria-valuemin="0" aria-valuemax="100">1%</div>
</div>

<script>

    GoToSelectTable();
    function GoToSelectTable() {
        var progressBar = $('#errorPageProgressBar'),width = 0;

        var interval = setInterval(function () {
            if (width < 30) {
                width += 5;

            }
            else {
                width += 10;
            }
            progressBar.css('width', width + '%');
            progressBar.html(width + '%');

            if (width >= 100) {
                clearInterval(interval);
                $.post("/NumuneFormu/NewTableModal", { tabloTuru: 1 }, function (deger) {
                    $('#stepContainer').html(deger);
                    SetStepBreadCrumb(5);

                }).fail(function (response) {
                    alert('Error : ' + response.responseText);
                });
            }
        }, 100);

        //Buradaki post methodunu düzelt bir sonraki aşamaya gitmiyor.
       
    }
</script>