     }


    var kumasKonstruksiyonList = []

    function AddKonstruksiyon() {
        var seciliDeger = $('#kumasKonstruksiyon').val();

        var konstruksiyonHtmlString = '<li> ' +
            '<div class="form-row">' +
            '<div class="form-group col-md-5">' +
            '<input type="text" readonly id="' + seciliDeger + 'Parameter" value="' + seciliDeger + '"/>' +
            '</div>' +
            '<div class="form-group col-md-5">' +
            '<input type="number" id="' + seciliDeger + 'Value" onchange="ValueChanged()" value="0" />' +
            '</div>' +
            '<div class="form-group col-md-2">' +
            '<a href="#" class="btn btn-icon btn-danger" data-konstruksiyon="' + seciliDeger + '" onclick="RemoveKonstruksiyon(this)"><i class="fas fa-trash" style="color:white"></i></a>' +
            '</div>' +
            '</div>' +
            '</li>';

        $('#konstruksiyonInputs').append(konstruksiyonHtmlString);
    }

    function RemoveKonstruksiyon(elem) {
        var seciliDeger = $(elem).data('konstruksiyon');
        $(elem).parent().parent().parent().remove();
    }

    function ValueChanged() {
        var sumInsıde = 0;
        $('#konstruksiyonInputs').find('li').each(function () {
            var seciliDeger = parseFloat($(this).children().find('input[type="number"]').val());

            sumInsıde += seciliDeger;

            if (sumInsıde > 100) {
                $(this).children().find('input[type="number"]').val(0);
                alert("Toplam 100'ü geçemez");
            }
        });
    }

